# Database Creds & Settings
DB_NAME=db
DB_USER=user
DB_PASSWORD=pass
DB_HOST=localhost
DB_CHARSET=utf8
DB_COLLATE=
WP_ALLOW_REPAIR=true
CUSTOM_USER_TABLE=users
CUSTOM_USER_META_TABLE=usermeta
DB_TABLE_PREFIX=wp_

# Debugging
DISPLAY_ERRORS=On
LOG_ERRORS=Off
WP_DEBUG=true
WP_DEBUG_LOG=false
WP_DEBUG_DISPLAY=true
SAVEQUERIES=true
ERRORLOGFILE=/log/error.log
DIEONDBERROR=true

# Language Settings
WPLANG=en_EN
WP_LANG_DIR={$WP_CONTENT_DIR}/tmp
LANGDIR={$WP_LANG_DIR}

# Paths & URls
WP_SITEURL=http://example.dev
WP_HOME=WP_SITEURL
WP_CONTENT_DIR={$ABSPATH}/wp-content
