# Filesystem Settings
FS_CHMOD_DIR=0755
FS_CHMOD_FILE=0644
FS_METHOD
FS_TIMEOUT=30
FS_CONNECT_TIMEOUT=30

# Proxy Settings
WP_PROXY_HOST
WP_PROXY_PORT
WP_PROXY_USERNAME
WP_PROXY_PASSWORD
WP_PROXY_BYPASS_HOSTS
WP_ACCESSIBLE_HOSTS
WP_HTTP_BLOCK_EXTERNAL

